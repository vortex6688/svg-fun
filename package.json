{
  "name": "tn-admin-editorial",
  "version": "0.1.0",
  "description": "It's admin and editorial frontends!",
  "keywords": [
    "angular2",
    "webpack",
    "typescript",
    "schmeckles"
  ],
  "author": "Conrad Kreyling <conrad@pillowfight.io>",
  "homepage": "https://github.com/TypeNetwork/tn-admin-editorial",
  "license": "Proprietary",
  "scripts": {
    "build": "webpack --config config/webpack.prod.js --bail",
    "build:verbose": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build:docker": "npm run build && docker build -t angular2-webpack-start:latest .",
    "ci": "npm run clean:build && npm run lint && npm run test && npm run build && npm run e2e",
    "clean:build": "npm-run-all --parallel clean:dist clean:dll clean:aot",
    "clean:dll": "npm run rimraf -- dll",
    "clean:aot": "npm run rimraf -- compiled",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean": "npm cache clean && npm run rimraf -- doc coverage dist compiled dll",
    "codecov": "codecov",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm-run-all -p -r server:dist protractor:live",
    "e2e": "npm-run-all -p -r server:dist protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postinstall": "npm run webdriver:update",
    "postversion": "git push && git push --tags",
    "preclean:install": "npm run clean",
    "preversion": "npm test",
    "protractor": "protractor",
    "protractor:live": "protractor --elementExplorer",
    "rimraf": "rimraf",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --inline --hot --content-base src/",
    "server:dist": "superstatic -c ./superstatic.json dist --port 3000",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch": "npm run test -- --auto-watch --no-single-run",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
